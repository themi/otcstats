<p id="notice"><%= notice %></p>

<h1>Statistics</h1>

<table>
  <thead>
    <tr>
      <th>Organisation</th>
      <th>Graph</th>
      <th>Added by</th>
      <th>Week ending at</th>
      <th>Value</th>
      <th>Fact column 1</th>
      <th>Fact column 2</th>
      <th>Fact column 3</th>
      <th>Fact column 4</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @statistics.each do |statistic| %>
      <tr>
        <td><%= statistic.organisation_id %></td>
        <td><%= statistic.graph_id %></td>
        <td><%= statistic.added_by %></td>
        <td><%= statistic.week_ending_at %></td>
        <td><%= statistic.value %></td>
        <td><%= statistic.fact_column_1 %></td>
        <td><%= statistic.fact_column_2 %></td>
        <td><%= statistic.fact_column_3 %></td>
        <td><%= statistic.fact_column_4 %></td>
        <td><%= link_to 'Show', statistic %></td>
        <td><%= link_to 'Edit', edit_statistic_path(statistic) %></td>
        <td><%= link_to 'Destroy', statistic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Statistic', new_statistic_path %>
